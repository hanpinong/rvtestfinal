extends layout

block content

    div
        a(id="back", href="/") Go back

    div
        h2 NVLCP0 RV Partition Indicators
        h3 Last Updated at : #{lastupdate}
        h3 Data extracted from : #{datapath}

    div
        input(type='text', id="rawsearch" ,placeholder= "Type to search...")
        button.btn.btn-primary(id="clear_raw_filter", type='button') Clear Filter 

    div

        table.table.table-striped(id="tablervnvlcp0")
            thead
                tr
                    th.indicators(style="width:135px") Partition
                    - var table1count
                    - table1count = 0
                    - console.log(Tools)
                    for tool in Tools
                        - table1count = table1count + 1
                        - console.log(tool)
                        if tool.indexOf("Ungated") != -1
                            - var tool_disp = tool.slice(14)
                            th.indicators UDVD >= #{tool_disp}
                        else if tool.indexOf("Gated") != -1
                            - var tool_disp = tool.slice(13)
                            th.indicators GDVD >= #{tool_disp}
                        else if tool == 'missing AON VL'
                            pass
                        else if tool == 'missing AON v4'
                            pass
                        else if tool == 'latest_dbtag'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:185px',onclick="sortTable("+table1count+")") #{tool}
                        else if tool == 'rv_dbtag'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:185px',onclick="sortTable("+table1count+")") #{tool}
                        else if tool == 'rv_checkin_date'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:285px',onclick="sortTable("+table1count+")") #{tool}
                        else if tool == 'latest_checkin_date'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:285px',onclick="sortTable("+table1count+")") #{tool}
                        else if tool == 'pg_opens'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:105px',onclick="sortTable("+table1count+")") #{tool}
                        else if tool == 'stair4p5'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:120px',onclick="sortTable("+table1count+")") StaIR => 4.5%
                        else if tool == 'stair_worstcase'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:95px',onclick="sortTable("+table1count+")") StaIR_WC
                        else if tool == 'dynir'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:120px',onclick="sortTable("+table1count+")") DynIR => 15%
                        else if tool == 'dvd_worstcase'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:95px',onclick="sortTable("+table1count+")") DynIR_WC
                        
                        else if tool == 'dynir15ungated'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:120px',onclick="sortTable("+table1count+")") UnGated DynIR => 15%
                        else if tool == 'dynir15gated'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:120px',onclick="sortTable("+table1count+")") Gated DynIR => 15%
                        else if tool == 'dynir20gated'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:120px',onclick="sortTable("+table1count+")") A0 Gated DynIR => 20%
                        else if tool == 'thermal.130PWR.metal.AVG'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:140px',onclick="sortTable("+table1count+")") A0_thermal.PWR.metal.AVG >130%
                        else if tool == 'thermal.130PWR.via.AVG'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:140px',onclick="sortTable("+table1count+")") A0_thermal.PWR.via.AVG >130%

                        else if tool == 'pgem.PWR.metal.PEAK'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:110px',onclick="sortTable("+table1count+")") #{tool} >100%
                        else if tool == 'pgem.PWR.metal.PEAK_worstcase'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:110px',onclick="sortTable("+table1count+")") pgem.PWR.metal.PEAK worstcase
                        else if tool == 'thermal.PWR.metal.AVG'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:115px',onclick="sortTable("+table1count+")") #{tool} >100%
                        else if tool == 'thermal.PWR.metal.AVG_worstcase'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:115px',onclick="sortTable("+table1count+")") thermal.PWR.metal.AVG worstcase
                        else if tool == 'thermal.PWR.via.AVG'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:115px',onclick="sortTable("+table1count+")") #{tool} >100%
                        else if tool == 'thermal.PWR.via.AVG_worstcase'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:115px',onclick="sortTable("+table1count+")") thermal.PWR.via.AVG worstcase
                        else if tool == 'sigem.SIG.metal.PEAK'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:105px',onclick="sortTable("+table1count+")") #{tool} >100%
                        else if tool == 'sigem.SIG.metal.PEAK_worstcase'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:105px',onclick="sortTable("+table1count+")") sigem.SIG.metal.PEAK worstcase
                        else if tool == 'thermal.SIG.metal.AVG'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:110px',onclick="sortTable("+table1count+")") #{tool} >100%
                        else if tool == 'thermal.SIG.metal.AVG_worstcase'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:110px',onclick="sortTable("+table1count+")") thermal.SIG.metal.AVG worstcase
                        else if tool == 'thermal.SIG.via.AVG'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:110px',onclick="sortTable("+table1count+")") #{tool} >100%
                        else if tool == 'thermal.SIG.via.AVG_worstcase'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:110px',onclick="sortTable("+table1count+")") thermal.SIG.via.AVG worstcase
                        else if tool == 'thermal.PWR.out.SH'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:120px',onclick="sortTable("+table1count+")") #{tool} >10C
                        else if tool == 'thermal.PWR.out.SH_worstcase'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:120px',onclick="sortTable("+table1count+")") thermal.PWR.out.SH worstcase
                        else if tool == 'thermal.SIG.out.SH'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:110px',onclick="sortTable("+table1count+")") #{tool} >10C
                        else if tool == 'thermal.30j.SIG.out.SH'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:110px',onclick="sortTable("+table1count+")") thermal.SIG.out.SH >30C
                        else if tool == 'thermal.SIG.out.SH_worstcase'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:110px',onclick="sortTable("+table1count+")") thermal.SIG.out.SH worstcase
                        else if tool == 'thermal.INST.out.SH'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:110px',onclick="sortTable("+table1count+")") #{tool} >25C
                        else if tool == 'thermal.INST.out.SH_worstcase'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:110px',onclick="sortTable("+table1count+")") thermal.INST.out.SH worstcase
                        else if tool == 'thermal.PWR.out.TEMP'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:120px',onclick="sortTable("+table1count+")") #{tool} >150C
                        else if tool == 'thermal.PWR.out.TEMP_worstcase'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:120px',onclick="sortTable("+table1count+")") thermal.PWR.out.TEMP worstcase
                        else if tool == 'thermal.SIG.out.TEMP'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:110px',onclick="sortTable("+table1count+")") #{tool} >150C
                        else if tool == 'thermal.SIG.out.TEMP_worstcase'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:110px',onclick="sortTable("+table1count+")") thermal.SIG.out.TEMP worstcase
                        
                        else if tool == '120rescheck'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:105px',onclick="sortTable("+table1count+")") #{tool}
                        else if tool == 'rescheck_worstcase'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:155px',onclick="sortTable("+table1count+")") #{tool}
                        
                        else if tool == 'apl_cap_check'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:120px',onclick="sortTable("+table1count+")") #{tool}
                        else if tool == 'apl_current_check'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:145px',onclick="sortTable("+table1count+")") #{tool}
                        else if tool == 'lef_check'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:95px',onclick="sortTable("+table1count+")") #{tool}
                        else if tool == 'lib_check'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:95px',onclick="sortTable("+table1count+")") #{tool}
                        else if tool == 'spef_check'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:95px',onclick="sortTable("+table1count+")") #{tool}
                        else if tool == 'timing_window'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:135px',onclick="sortTable("+table1count+")") #{tool}
                        else if tool == 'timing_slew'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:105px',onclick="sortTable("+table1count+")") #{tool}
                        else if tool == 'ward'
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:585px',onclick="sortTable("+table1count+")") #{tool}
                        else
                            th.indicators(data-toogle="tooltip", title=tooltip_mapping[tool], style = 'width:105px') #{tool}

            tbody
                for key, part in testing
                    tr
                        td #{part}
    
                        for tool in Tools
                            - var val 
                            - if(key[tool] < 0){
                            -    val = 0
                            - }
                            - else{
                            -    val = key[tool]
                            - }
                            if tool.indexOf('drcdf_base1') != -1 & val > 0
                                td
                                    a(href="/rvnvlcp0violations/loadviolation/?partition="+part+"&viol_type="+"drcdf_base1", data-toogle="tooltip", title=part + " " + tool) #{val}
                            else if tool.indexOf('drcdf_base2') != -1 & val > 0
                                td
                                    a(href="/rvnvlcp0violations/loadviolation/?partition="+part+"&viol_type="+"drcdf_base2", data-toogle="tooltip", title=part + " " + tool) #{val}
                            else if tool.indexOf("complete_date") != -1
                                td #{val}
                            else if tool.indexOf("ward") != -1
                                td #{val}
                            else if tool.indexOf("_wc") != -1
                                td #{val}
                            else if tool.indexOf("nlib_source") != -1
                                td #{val}
                            else if tool.indexOf("dbtag") != -1
                                td #{val}
                            else if tool.indexOf("checkin_date") != -1
                                td #{val}
                            else if tool.indexOf("Gated") != -1 & val > 0
                                td
                                    a(href="/rvnvlcp0violations/loadviolation/?partition="+part+"&viol_type="+"gdvd", data-toogle="tooltip", title="View violations") #{val}
                            else if tool.indexOf("Ungated") != -1 & val > 0
                                td
                                    a(href="/rvnvlcp0violations/loadviolation/?partition="+part+"&viol_type="+"udvd", data-toogle="tooltip", title="View violations") #{val}
                            else if tool.indexOf('worstcase') != -1 
                                td #{val}
                            else if tool == 'missing AON VL'
                                pass
                            else if tool == 'missing AON v4'
                                pass
  
                            else if tool == 'HIP No Power'
                                pass
                            else if tool == 'reused'
                                td #{val}
                            else if tool == 'fit_value'
                                td #{val}
                            else if tool == 'ir_md_def'
                                td #{val}
                            else if tool == 'em_md_def'
                                td #{val}

                            else if tool == 'HIP <50% vias'
                                pass 

                            else if tool == 'unbounded AON'
                                pass

                            else if tool == 'glbdrv v6 missing'
                                pass
                            else if tool == 'glbdrv v5 missing'
                                pass
                            else
                                td 
                                    a(href="/rvnvlcp0violations/loadviolation/?partition="+part+"&viol_type="+tool, data-toogle="tooltip", title=part + " " + tool) #{val}

    script(type='text/javascript', src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js")
    script(type="text/javascript", src="https://milankyncl.github.io/jquery-copy-to-clipboard/jquery.copy-to-clipboard.js")
    script(src="searchfilterrv.js")
    script(src='copytext.js')
    script(src="../loadviolations.js")
    script.
        $('[data-toggle="tooltip"]').tooltip(); 


